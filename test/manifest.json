{
  "manifest_version": 2,
  "name": "Verify Extension (Testing)",
  "author": " ",
  "version": "1.0.0",
  "description": "An extension to automatically
    fill verification codes. (Tests)",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "test.html"
  },
  "icons": {
    "48": "img/icon48x48.png",
    "128": "img/icon128x128.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "contextMenus",
    "tabs"
  ]
}
